{
    "name": "Asciidoc Book Addon",
    "description": "This extension is made for Asciidoc Book",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
       "activeTab",
       "scripting"
    ],
    "background": {
       "service_worker": "background.js"
    },
    "action": {
       "default_popup": "popup.html"
    }
 }